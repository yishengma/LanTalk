    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <android.support.v4.view.ViewPager
        android:clipChildren="false"
        android:scrollbars="none"
        android:layout_gravity="center"
        android:layout_marginRight="@dimen/dimen_margin_sixty"
        android:layout_marginLeft="@dimen/dimen_margin_sixty"
        android:id="@+id/vp_head_portrait"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_pick_height">

    </android.support.v4.view.ViewPager>
</LinearLayout>